public class task12 {

	public static void main(String[] args) {
		int a=10;
		int b=0;
		try {
			int c=divide(a,b);
			System.out.println(c);
		}catch(ArithmeticException e) {
			System.out.println("Error : Element can not divide by zero");
		}
	}
	private static int divide(int a, int b) {
		return a/b;
	}
}
